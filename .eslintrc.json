{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "plugins": [
    "react-hooks"
  ],
  "extends": "airbnb",
  "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
